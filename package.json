{
  "private": true,
  "name": "utilize-schema",
  "description": "A collection of utilities for working with JSON Schema",
  "keywords": [
    "schema",
    "json-schema",
    "utilities",
    "parser",
    "validation",
    "generator"
  ],
  "license": "MIT",
  "author": {
    "name": "Matej Bransky",
    "email": "matej.bransky@gmail.com"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "check:format": "pnpm run --recursive --parallel --aggregate-output --reporter-hide-prefix --if-present check:format",
    "check:lint": "pnpm run --recursive --parallel --aggregate-output --reporter-hide-prefix --if-present check:lint",
    "check:types": "pnpm run --recursive --parallel --aggregate-output --reporter-hide-prefix --if-present check:types",
    "format": "pnpm run --recursive --parallel --aggregate-output --if-present format",
    "ci:test": "pnpm run --recursive --parallel --aggregate-output --reporter-hide-prefix --if-present ci:test",
    "ci:version": "pnpm changeset version",
    "ci:publish": "pnpm changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.4"
  },
  "packageManager": "pnpm@10.6.5"
}
