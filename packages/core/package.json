{
	"name": "@utilize/json-schema-core",
	"version": "0.0.1",
	"description": "Core utilities for working with JSON Schema.",
	"keywords": [
		"dereferencer",
		"linker",
		"normalizer"
	],
	"license": "MIT",
	"author": {
		"name": "Matej Bransky",
		"email": "matej.bransky@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:MatejBransky/utilize-schema.git",
		"directory": "packages/core"
	},
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"scripts": {
		"prepublishOnly": "pnpm build",
		"build": "unbuild",
		"stub": "unbuild --stub",
		"format": "pnpm check:format --write",
		"check:format": "prettier --check ./{src,test}/**/*.ts",
		"check:lint": "eslint .",
		"check:types": "tsc --noEmit",
		"test": "vitest",
		"ci:test": "vitest run"
	},
	"packageManager": "pnpm@10.6.5",
	"devDependencies": {
		"@total-typescript/tsconfig": "1.0.4",
		"@types/json-schema": "7.0.15",
		"@utilize/eslint-config": "3.0.1",
		"@utilize/prettier-config": "1.1.3",
		"eslint": "9.25.0",
		"prettier": "3.5.3",
		"typescript": "5.8.3",
		"unbuild": "3.5.0",
		"vitest": "3.2.2"
	},
	"prettier": "@utilize/prettier-config",
	"dependencies": {
		"@apidevtools/json-schema-ref-parser": "^13.0.1"
	}
}
