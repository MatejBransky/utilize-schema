{
	"name": "@utilize/zod",
	"version": "0.4.0",
	"description": "Zod utilities for working with JSON Schema.",
	"keywords": [
		"parser",
		"optimizer",
		"ast",
		"generator"
	],
	"license": "MIT",
	"author": {
		"name": "Matej Bransky",
		"email": "matej.bransky@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:MatejBransky/utilize-schema.git",
		"directory": "packages/core"
	},
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"scripts": {
		"prepublishOnly": "pnpm build",
		"build": "unbuild",
		"build:dev": "unbuild --stub",
		"stub": "unbuild --stub",
		"format": "prettier . --write && eslint --fix .",
		"check:format": "prettier . --check",
		"check:lint": "eslint .",
		"check:types": "tsc --noEmit",
		"test": "vitest",
		"ci:test": "vitest run"
	},
	"packageManager": "pnpm@10.19.0",
	"dependencies": {
		"@types/toposort": "2.0.7",
		"@utilize/json-schema": "workspace:*",
		"toposort": "2.0.2",
		"zod": "4.1.12"
	},
	"devDependencies": {
		"@apidevtools/json-schema-ref-parser": "^14.2.1",
		"@total-typescript/tsconfig": "1.0.4",
		"@types/lodash": "^4.17.20",
		"@types/node": "^22.0.0",
		"@utilize/eslint-config": "3.0.1",
		"@utilize/prettier-config": "1.1.3",
		"eslint": "^9.38.0",
		"lodash": "4.17.21",
		"prettier": "3.6.2",
		"typescript": "~5.9.3",
		"unbuild": "3.6.1",
		"vitest": "4.0.3"
	},
	"prettier": "@utilize/prettier-config"
}
