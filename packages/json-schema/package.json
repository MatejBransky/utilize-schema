{
	"name": "@utilize/json-schema",
	"version": "0.1.0",
	"description": "Utilities for working with JSON Schema.",
	"keywords": [
		"linker",
		"normalizer",
		"parser",
		"optimizer"
	],
	"license": "MIT",
	"author": {
		"name": "Matej Bransky",
		"email": "matej.bransky@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:MatejBransky/utilize-schema.git",
		"directory": "packages/json-schema"
	},
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"scripts": {
		"prepublishOnly": "pnpm build",
		"build": "unbuild",
		"build:dev": "unbuild --stub",
		"format": "prettier . --write && eslint --fix .",
		"check:format": "prettier . --check",
		"check:lint": "eslint .",
		"check:types": "tsc --noEmit",
		"test": "vitest",
		"ci:test": "vitest run"
	},
	"packageManager": "pnpm@10.19.0",
	"devDependencies": {
		"@total-typescript/tsconfig": "1.0.4",
		"@types/json-schema": "7.0.15",
		"@types/node": "^22.0.0",
		"@utilize/eslint-config": "3.0.1",
		"@utilize/prettier-config": "1.1.3",
		"eslint": "9.38.0",
		"prettier": "3.6.2",
		"typescript": "5.9.3",
		"unbuild": "3.6.1",
		"vitest": "4.0.3"
	},
	"prettier": "@utilize/prettier-config",
	"dependencies": {
		"@apidevtools/json-schema-ref-parser": "^14.2.1",
		"dequal": "2.0.3",
		"moize": "6.1.6"
	}
}
